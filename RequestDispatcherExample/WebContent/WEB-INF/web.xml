<web-app>
  <display-name>RequestDispatcherExample</display-name>
  <welcome-file-list>
    <welcome-file>/pages/index.html</welcome-file>
  </welcome-file-list>
  <servlet>
  <servlet-name>StudentServlet</servlet-name>
  <servlet-class>com.requestdispatcherexample.controller.StudentServlet
  </servlet-class>
  <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
  <servlet-name>StudentServlet</servlet-name>
  <url-pattern>/pages/searchStudent</url-pattern>
  </servlet-mapping>
  
   <servlet>
  <servlet-name>ViewStudentServlet</servlet-name>
  <servlet-class>com.requestdispatcherexample.controller.ViewStudentServlet
  </servlet-class>
  <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
  <servlet-name>ViewStudentServlet</servlet-name>
  <url-pattern>/viewStudentServlet</url-pattern>
  </servlet-mapping>
  
</web-app>